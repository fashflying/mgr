<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="mgr.wfengine.domain">
	<class name="FlowData" table="flowdata">
		<id name="id" column="id">
			<generator class="uuid"></generator>
		</id>
		
		<property name="dataName" type="string"></property>
		<property name="dataValue" type="string"></property>
		<property name="lastUpdate" type="timestamp"></property>
		<many-to-one name="flowInstance" column="flowInstanceId" class="FlowInstance"></many-to-one>
		<many-to-one name="flow" column="flowId" class="Flow"></many-to-one>
		
	</class>
</hibernate-mapping>