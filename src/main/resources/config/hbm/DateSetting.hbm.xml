<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="mgr.wfengine.domain">
	<class name="DateSetting" table="DateSetting">
		<id name="id">
			<generator class="uuid"></generator>
		</id>
		<property name="extraDate" type="date" not-null="true"></property>
		<property name="extraTime" type="time"></property>
		<property name="property" type="string" not-null="true"></property>
		<property name="remark" type="string"></property>
	</class>
</hibernate-mapping>